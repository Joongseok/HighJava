<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="basic.childWinTest.MainWinController">
   <children>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="300.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="50.0" text="ID" />
          <TableColumn fx:id="nameCol" prefWidth="100.0" text="이름" />
            <TableColumn fx:id="addrCol" prefWidth="150.0" text="주소" />
        </columns>
      </TableView>
      <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#btnAddClicked" text="데이터 추가">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
