<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="304.0" prefWidth="350.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="basic.student.StudentMainController">
   <children>
      <TableView fx:id="table" onMouseClicked="#tableClicked" prefHeight="276.0" prefWidth="350.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="이름" />
          <TableColumn fx:id="gukCol" prefWidth="75.0" text="국어" />
            <TableColumn fx:id="suCol" prefWidth="75.0" text="수학" />
            <TableColumn fx:id="engCol" prefWidth="75.0" text="영어" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" spacing="20.0" VBox.vgrow="SOMETIMES">
         <children>
            <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#btnAddClicked" text="추가" />
            <Button fx:id="btnBarChart" mnemonicParsing="false" onAction="#btnBarChartClicked" text="학생별 막대그래프" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
